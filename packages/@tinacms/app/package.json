{
  "name": "@tinacms/app",
  "version": "0.0.12",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.js"
    }
  },
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "script.mjs",
    "package.json",
    "public",
    "appFiles",
    "tsconfig.json",
    "tsconfig.node.json",
    "vite.config.ts"
  ],
  "license": "Apache-2.0",
  "buildConfig": {
    "entryPoints": [
      {
        "name": "src/index.ts",
        "target": "node"
      }
    ]
  },
  "scripts": {
    "types": "rm dist/index.d.ts && touch dist/index.d.ts && echo \"export declare const viteBuild: (args: any) => any\" > dist/index.d.ts",
    "build": "tinacms-scripts build"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.1",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.6",
    "@heroicons/react": "^1.0.6",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@tailwindcss/typography": "^0.5.4",
    "@tinacms/schema-tools": "workspace:*",
    "@tinacms/scripts": "workspace:*",
    "@vitejs/plugin-react": "1.3.2",
    "@xstate/react": "^3.0.0",
    "autoprefixer": "^10.4.0",
    "final-form": "^4.20.7",
    "fs-extra": "^9.0.1",
    "esbuild": "^0.15.5",
    "graphql": "^15.1.0",
    "jest": "^27.0.6",
    "postcss": "^8.4.14",
    "postcss-nested": "^5.0.6",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "6",
    "tailwindcss": "^3.1.6",
    "tinacms": "workspace:*",
    "typescript": "^4.6.4",
    "vite": "^2.4.3",
    "vite-node": "^0.18.1",
    "xstate": "^4.32.1"
  }
}
